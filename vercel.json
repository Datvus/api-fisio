{
  "version": 2,
  "builds": [{ "src": "api/*.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/api/permission", "dest": "/api/listPermission.js"},
    { "src": "/api/all/permission", "dest": "/api/listPermissions.js"},
    { "src": "/api/create/permission", "dest": "/api/createPermission.js"},
    { "src": "/api/update/permission", "dest": "/api/updatePermission.js"}
  ]
}